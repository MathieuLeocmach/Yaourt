month = apr,
number = {4},
pages = {293--7},
pmid = {15750598},
title = {{Nested self-similar wrinkling patterns in skins.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/15750598},
volume = {4},
year = {2005}
}
@article{Yampolskaya2006,
